<Window x:Class="ASAP_WPF.LengthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASAP_WPF"
        mc:Ignorable="d"
        Title="Length Window" Height="800" Width="450"
        Closing="Window_Closing">
    <Grid>
        <DataGrid Name="LengthGrid"  DockPanel.Dock="Right"  ItemsSource="{Binding TempLengthList }" ColumnWidth="Auto"/>
    </Grid>
</Window>
